        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-faker": "^1.0",
        "loophp/phpunit-iterable-assertions": "^1.0",
        "doctrine/collections": "^1.6",
        "phpstan/phpstan": "^1.6",
        "phpmd/phpmd": "^2.12"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}